{
	"name": "Auto Get Bonus",
	"description": "Module permettant de recuperer automatiquement les gifts de points de chaine twitch",
	"version": "1.1.0",
	"permissions": ["activeTab"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_title": "Auto Get Twitch Gift"
	},
	"icons": {
		"64": "icon/Gift.png"
	},
	"manifest_version": 2
}
